{
	"name": "SSD",
	"no_GPUs": 1,
	"model": {
		"type": "CNN_SSD",
		"args": {
			"learnable": false
		}
	},
	"data_loss": {
		"type": "SSD"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 1.4
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},
	"transformation_module": {
		"type": "SVF_3D",
		"args": {
			"no_steps": 12
		}
	},
	"registration_module": {
		"type": "RegistrationModule"
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.003
		}
	},
	"data_loader": {
		"type": "LearnSimDataLoader",
		"args": {
			"batch_size": 3,
			"data_dir": "/vol/bitbucket/dig15/datasets/mine/biobank/biobank_120",
			"dims": [
				128,
				128,
				128
			],
			"num_workers": 3,
			"rescale_im": true,
			"sigma_v_init": 0.5,
			"u_v_init": 0.0
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 7,
		"lambda": 0.5
	},
	"trainer": {
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"no_epochs": 1,
		"no_iters_q_v": 2048,
		"log_period": 1024,
		"var_params_backup_period": 5,
		"no_samples_test": 50,
		"tensorboard": true,
		"save_dir": "saved/",
		"verbosity": 2
	}
}
