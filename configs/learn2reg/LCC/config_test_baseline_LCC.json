{
	"name": "learn2reg",
	"model": {
		"type": "CNN_LCC",
		"args": {
			"learnable": false,
			"s": 1
		}
	},
	"data_loss": {
		"type": "LCC"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 0.6
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},
	"transformation_module": {
		"type": "SVFFD_3D",
		"args": {
			"cps": [3, 3, 3]
		}
	},
	"registration_module": {
		"type": "RegistrationModule"
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.02
		}
	},
	"data_loader": {
		"type": "Learn2RegDataLoader",
		"args": {
			"im_pairs": "/vol/biodata/data/learn2reg/2021/task03/pairs_val.csv",
			"dims": [160, 192, 224],
			"sigma_v_init": 0.01,
			"u_v_init": 0.01,
			"batch_size": 1,
			"no_im_pairs_per_epoch": 50,
			"no_workers": 2,
			"test": true
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 3,
		"lambda": 0.5
	},
	"trainer": {
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"no_epochs": 1,
		"no_iters_q_v": 1280,
		"no_samples_SGLD": 2000,
		"no_samples_SGLD_burn_in": 1000,
		"no_samples_test": 5,
		"save_dir": "saved/"
	}
}
