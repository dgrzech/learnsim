{
	"name": "learn2reg",
	"model": {
		"type": "CNN_SSD",
		"args": {
			"learnable": false
		}
	},
	"data_loss": {
		"type": "SSD"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 20.0
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},
	"transformation_module": {
		"type": "SVF_3D",
		"args": {
			"no_steps": 12
		}
	},
	"registration_module": {
		"type": "RegistrationModule"
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.02
		}
	},
	"data_loader": {
		"type": "Learn2RegDataLoader",
		"args": {
			"im_pairs": "/vol/biodata/data/learn2reg/2021/task03/pairs_val.csv",
			"dims": [160, 192, 224],
			"sigma_v_init": 0.05,
			"u_v_init": 0.05,
			"batch_size": 10,
			"no_im_pairs_per_epoch": 500,
			"no_workers": 10,
			"test": true
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 3,
		"lambda": 0.5
	},
	"trainer": {
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"w_reg_energy": 0.001,
		"no_epochs": 1,
		"no_iters_q_v": 1280,
		"no_samples_SGLD": 2000,
		"no_samples_test": 5,
		"save_dir": "saved/"
	}
}
