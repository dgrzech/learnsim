{
	"name": "SSD",
	"no_GPUs": 2,
	"model": {
		"type": "CNN_SSD",
		"args": {
			"learnable": true,
			"no_features": [4, 8, 8],
			"activation": {
				"type": "LeakyReLU",
				"args": {
					"negative_slope": 0.2
				}
			}
		}
	},
	"data_loss": {
		"type": "SSD"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 1.8
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},
	"transformation_module": {
		"type": "SVF_3D",
		"args": {
			"no_steps": 12
		}
	},
	"registration_module": {
		"type": "RegistrationModule"
	},
	"optimizer_LD": {
		"type": "SGD",
		"args": {
			"lr": 0.0001
		}
	},
	"optimizer_q_phi": {
		"type": "Adam",
		"args": {
			"lr": 0.00001
		}
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.01
		}
	},
	"data_loader": {
		"type": "LearnSimDataLoader",
		"args": {
			"im_pairs": "/vol/bitbucket/dig15/datasets/mine/biobank/train_biobank_1500.csv",
			"dims": [128, 128, 128],
			"sigma_v_init": 0.5,
			"u_v_init": 0.1,
			"batch_size": 5,
			"no_workers": 5,
			"test": false
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 3,
		"lambda": 0.5
	},
	"trainer": {
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"no_epochs": 50,
		"no_iters_q_v": 1024,
		"no_samples_SGLD": 2000,
		"no_samples_SGLD_burn_in": 1000,
		"no_samples_test": 5,
		"log_period": 512,
		"log_period_model_samples": 100,
		"save_dir": "saved/"
	}
}
