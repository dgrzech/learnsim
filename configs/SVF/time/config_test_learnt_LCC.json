{
	"name": "time",
	"no_GPUs": 1,
	"model": {
		"type": "CNN_LCC",
		"args": {
			"learnable": true,
			"s": 1,
			"no_features": [4, 8, 8],
			"activation": {
				"type": "LeakyReLU",
				"args": {
					"negative_slope": 0.2
				}
			}
		}
	},
	"data_loss": {
		"type": "LCC"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 2.4
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},
	"transformation_module": {
		"type": "SVF_3D",
		"args": {
			"no_steps": 12
		}
	},
	"registration_module": {
		"type": "RegistrationModule"
	},
	"optimizer_LD": {
		"type": "SGD",
		"args": {
			"lr": 0.01
		}
	},
	"optimizer_q_phi": {
		"type": "Adam",
		"args": {
			"lr": 0.00001
		}
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.02
		}
	},
	"data_loader": {
		"type": "LearnSimDataLoader",
		"args": {
			"batch_size": 1,
			"data_dir": "/vol/bitbucket/dig15/datasets/mine/biobank/biobank_1350",
			"dims": [128, 128, 128],
			"sigma_v_init": 0.5,
			"u_v_init": 0.1,
			"no_workers": 1,
			"test": true,
			"test_split": 0.2
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 3,
		"lambda": 0.5
	},
	"trainer": {
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"no_epochs": 1,
		"no_iters_q_v": 1024,
		"no_samples_SGLD": 200,
		"no_samples_SGLD_burn_in": 100,
		"no_samples_test": 5,
		"log_period": 512,
		"log_period_model_samples": 100,
		"time_only": true,
		"save_dir": "saved/"
	}
}
