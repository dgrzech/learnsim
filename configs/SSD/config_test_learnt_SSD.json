{
	"name": "SSD",
	"no_GPUs": 1,
	"model": {
		"type": "CNN_SSD",
		"args": {
			"learnable": true,
			"no_features": [
				4,
				8,
				8
			],
			"activation": {
				"type": "LeakyReLU",
				"args": {
					"negative_slope": 0.2
				}
			}
		}
	},
	"q_f": {
		"type": "LowRankMultivariateNormalDistribution",
		"args": {
			"sigma_init": 0.1,
			"u_init": 0.1,
			"loc_learnable": false,
			"cov_learnable": true
		}
	},
	"data_loss": {
		"type": "SSD"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 1.4
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},
	"transformation_module": {
		"type": "SVF_3D",
		"args": {
			"no_steps": 12
		}
	},
	"registration_module": {
		"type": "RegistrationModule"
	},
	"optimizer_q_f": {
		"type": "Adam",
		"args": {
			"lr": 0.001
		}
	},
	"optimizer_q_phi": {
		"type": "Adam",
		"args": {
			"lr": 0.00001
		}
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 0.01
		}
	},
	"data_loader": {
		"type": "LearnSimDataLoader",
		"args": {
			"batch_size": 3,
			"data_dir": "/vol/bitbucket/dig15/datasets/mine/biobank/biobank_30",
			"dims": [
				128,
				128,
				128
			],
			"num_workers": 3,
			"rescale_im": true,
			"shuffle": false,
			"sigma_v_init": 0.5,
			"u_v_init": 0.1
		}
	},
	"Sobolev_grad": {
		"enabled": true,
		"s": 3,
		"lambda": 0.5
	},
	"trainer": {
		"uniform_noise": {
			"enabled": true,
			"magnitude": 0.1
		},
		"no_epochs": 1,
		"no_iters_q_v": 1024,
		"log_period": 512,
		"var_params_backup_period": 10,
		"no_samples_test": 50,
		"tensorboard": true,
		"save_dir": "saved/",
		"verbosity": 2
	}
}
