{
	"name": "temp",
	"n_gpu": 1,

	"optimize_q_v": true,
	"optimize_q_f": true,
	"optimize_q_phi": true,

	"model": {
        "type": "CNN_SSD",
        "args": {
			"learnable": true,
	    	"no_feature_maps": 8,
			"s": 3
	    }
    },

	"data_loss": {
		"type": "SSD"
	},
	"reg_loss": {
		"type": "RegLoss_L2",
		"args": {
			"diff_op": "GradientOperator",
			"w_reg": 2.0
		}
	},
	"entropy_loss": {
		"type": "EntropyMultivariateNormal"
	},

	"transformation_model": {
		"type": "SVF_3D"
	},
	"registration_module": {
		"type": "RegistrationModule"
	},

	"optimizer_q_f": {
		"type": "Adam",
		"args": {
			"lr": 2e-3
		}
	},
	"optimizer_q_phi": {
		"type": "Adam",
		"args": {
			"lr": 1e-4
		}
	},
	"optimizer_q_v": {
		"type": "Adam",
		"args": {
			"lr": 2e-3
		}
	},

	"data_loader": {
		"type": "LearnSimDataLoader",
		"args": {
			"batch_size": 4,
			"data_dir": "/vol/bitbucket/dig15/datasets/mine/biobank/biobank_08",
			"dims": [64, 64, 64]
		}
	},

	"Sobolev_grad": {
		"enabled": true,
		"s": 7,
		"lambda": 5e-1
	},

	"trainer": {
		"uniform_noise": {
			"enabled": false
		},

		"no_epochs": 4,
		"no_iters_q_v": 1024,
		"log_period": 128,

		"tensorboard": true,
		"verbosity": 2,
		"save_dir": "saved/"
	}
}
